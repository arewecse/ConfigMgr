  <DataSets>
    <DataSet Name="SCCMDeviceDetails">
      <Query>
        <DataSourceName>SCCMDataSource</DataSourceName>
        <CommandText>
          
          WITH LastLoggedOnUser AS (
              SELECT SYSTEMUSER.ResourceID, SYSTEMUSER.SystemConsoleUser0,
              ROW_NUMBER() OVER (PARTITION BY SYSTEMUSER.ResourceID ORDER BY SYSTEMUSER.ResourceID DESC) AS rn
              FROM CM_STM.dbo.v_GS_SYSTEM_CONSOLE_USER SYSTEMUSER
          ),
          FinalOU AS (
              SELECT ResourceID, System_OU_Name0
              FROM (
                  SELECT SYSOU.ResourceID, SYSOU.System_OU_Name0,
                  ROW_NUMBER() OVER (PARTITION BY SYSOU.ResourceID ORDER BY LEN(SYSOU.System_OU_Name0) DESC) AS rn
                  FROM CM_STM.dbo.v_RA_System_SystemOUName SYSOU
              ) AS RankedOUs WHERE rn = 1
          ),
          IPAddresses AS (
              SELECT NETIP.ResourceID, STRING_AGG(NETIP.IP_Addresses0, ', ') AS IPAddressList
              FROM CM_STM.dbo.v_RA_System_IPAddresses NETIP
              WHERE NETIP.IP_Addresses0 IS NOT NULL
              AND NETIP.IP_Addresses0 NOT LIKE '169.254.%'
              AND NETIP.IP_Addresses0 NOT LIKE '%:%'
              GROUP BY NETIP.ResourceID
          )
          SELECT DISTINCT
              SYS.Name0 AS 'Computer Name',
              COMP.Manufacturer0 AS 'Manufacturer',
              COMP.Model0 AS 'Model',
              BIOS.SerialNumber0 AS 'Serial Number',
              BIOS.SMBIOSBIOSVersion0 AS 'BIOS Version',
              OS.Caption0 AS 'OS Version',
              OS.BuildNumber0 AS 'OS Build Number',
              CASE WHEN OS.CSDVersion0 IS NULL THEN OS.Version0 + '.' + OS.BuildNumber0
                   ELSE OS.Version0 + '.' + OS.BuildNumber0 + ' ' + OS.CSDVersion0 END AS 'Full OS Build',
              OS.InstallDate0 AS 'OS Install Date',
              SYS.Full_Domain_Name0 AS 'Domain',
              COALESCE(FinalOU.System_OU_Name0, 'No OU Found') AS 'OU Path',
              CASE WHEN CS.ClientActiveStatus = 1 THEN 'Active' ELSE 'Inactive' END AS 'Client Status',
              CS.LastActiveTime AS 'Last Communication Time',
              DATEDIFF(DAY, CS.LastActiveTime, GETDATE()) AS 'Days Inactive',
              COALESCE(IPAddresses.IPAddressList, 'No IP Found') AS 'IP Address',
              LastLoggedOnUser.SystemConsoleUser0 AS 'Last Logged On User SID',
              USERINFO.Mail0 AS 'User Email'
          FROM CM_STM.dbo.v_R_System SYS
          LEFT JOIN CM_STM.dbo.v_GS_COMPUTER_SYSTEM COMP ON SYS.ResourceID = COMP.ResourceID
          LEFT JOIN CM_STM.dbo.v_GS_PC_BIOS BIOS ON SYS.ResourceID = BIOS.ResourceID
          LEFT JOIN CM_STM.dbo.v_GS_OPERATING_SYSTEM OS ON SYS.ResourceID = OS.ResourceID
          LEFT JOIN FinalOU ON SYS.ResourceID = FinalOU.ResourceID
          LEFT JOIN LastLoggedOnUser ON SYS.ResourceID = LastLoggedOnUser.ResourceID AND LastLoggedOnUser.rn = 1
          LEFT JOIN CM_STM.dbo.v_R_User USERINFO ON LastLoggedOnUser.SystemConsoleUser0 = USERINFO.Unique_User_Name0
          LEFT JOIN CM_STM.dbo.v_CH_ClientSummary CS ON SYS.ResourceID = CS.ResourceID
          LEFT JOIN IPAddresses ON SYS.ResourceID = IPAddresses.ResourceID
          ORDER BY SYS.Name0;
          
        </CommandText>
      </Query>
